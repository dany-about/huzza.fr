    <!-- Hero container -->
    <div class="container position-relative zindex-100">
      <div class="row row-grid justify-content-around align-items-center">
        <div class="col-lg-6 d-lg-block">
          <div class="text-center text-lg-left"">
            <h1 class="text-white mt-4 mb-3">Quand défier <br>devient un mode de vie !</h1>
            <p class="lead lh-180 text-white">Le premier réseau social qui te met au défi de réaliser ta vie.</p>
            <div class="mt-6">
              <a href="#exampleCards" class="btn btn-white btn-icon rounded-pill hover-translate-y-n3">
              <span class="btn-inner--icon"><i class="fas fa-angle-down"></i>
                <span class="btn-inner--text">Découvrir le concept</span>
              </a>
            </div>
          </div>
        </div>
        <div class="col-lg-6">
          <div class="card bg-section-secondary mt-6 mb-0 px-4 shadow-lg perspective-right hover-scale-110">
            <div class="card-body px-md-5 mt-0">
              <div class="text-center mx-auto mt-0 mb-3">
                <img alt="Image placeholder" src="https://image.noelshack.com/fichiers/2019/12/3/1553097422-huzza4.jpg" style="width: 230px">
              </div>
              <span class="clearfix"></span>
              <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                <%= render "devise/shared/error_messages", resource: resource %>
                <div class="form-group">
                  <div class="row">
                    <div class="col">
                      <label class="form-control-label" for="user_first_name">Prénom</label>
                      <%= f.text_field :first_name, class: 'form-control', placeholder: 'Mike', autofocus: true, autocomplete: "first name" %>
                    </div>
                    <div class="col">
                      <label class="form-control-label" for="user_last_name">Nom</label>
                      <%= f.text_field :last_name, class: 'form-control', placeholder: 'Horn', autofocus: true, autocomplete: "last name" %>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label class="form-control-label" for="input-email">Email</label>
                  <div class="input-group input-group-transparent">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fas fa-user"></i></span>
                    </div>
                    <%= f.email_field :email, class: 'form-control', id: 'input-email', placeholder: 'mike.horn@huzza.com', autofocus: true, autocomplete: "email" %>
                  </div>
                </div>
                <div class="form-group mb-4">
                  <label class="form-control-label" for="input-password">Mot de Passe</label>
                  <% if @minimum_password_length %>
                    <em>(<%= @minimum_password_length %> caractères minimum)</em>
                  <% end %><br />
                  <div class="input-group input-group-transparent">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fas fa-key"></i></span>
                    </div>
                    <%= f.password_field :password, class:"form-control", id:"input-password", placeholder:"********", autocomplete: "new-password" %>
                    <div class="input-group-append">
                    <span class="input-group-text" onclick="showPassword('input-password')">
                      <i class="fas fa-eye"></i>
                    </span>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label class="form-control-label" for="input-password-confirm">Confirmation</label>
                  <div class="input-group input-group-transparent">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fas fa-key"></i></span>
                    </div>
                    <%= f.password_field :password_confirmation,class:"form-control", id:"input-password-confirm", placeholder:"********", autocomplete: "new-password" %>
                  </div>
                </div>
                <div class="row align-items-center">
                  <div class="col-sm-7">
                    <%= f.submit "Rejoindre", class:"btn btn-dark mb-3 mb-sm-0" %>
                  </div>
                  <div class="col-sm-5 text-sm-right">
                    <div>
                      <span class="small d-sm-block d-md-inline">Déja un compte Huzza ?</span>
                      <%= link_to "Connexion",'/fr/users/sign_in', style:"small font-weight-bold"%>
                    </div>
                    <div>
                      <span class="small d-sm-block d-md-inline">ou</span><br>
                      <%= link_to "Facebook", user_facebook_omniauth_authorize_path, style:"small font-weight-bold"%>
                    </div>
                  </div>
                </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</div>

<div class="container" id="exampleCards">
  <% 5.times do |i| %>
  <div class="col-10 mx-auto" style="margin-top: 100px">
    <%= render "shared/dare_card", dare: Dare.all[i], friend: Dare.all[i].creator, occasion: nil, disclaimer: false %>
  </div>
  <% end %>
</div>

