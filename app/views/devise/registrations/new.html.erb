      <!-- registration -->
      <div class="container py-5">
        <div class="row justify-content-end">
            <div class="col-md-8 col-lg-5">
                <div class="card shadow zindex-100 mt-5">
                    <div class="card-body px-md-5 py-3 mt-5">
                        <div class="text-center mb-5">
                            <h6 class="h3">Create your free account</h6>
                            <p class="text-muted mb-0">to join the adventure</p>
                        </div>
                        <span class="clearfix"></span>
                        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                        <%= render "devise/shared/error_messages", resource: resource %>
                          <div class="form-group">
                            <div class="row">
                              <div class="col">
                                  <label class="form-control-label" for="user_first_name">First name</label>
                                  <%= f.text_field :first_name, class: 'form-control', placeholder: 'First name', autofocus: true, autocomplete: "first name" %>
                              </div>
                              <div class="col">
                                <label class="form-control-label" for="user_last_name">Last name</label>
                                <%= f.text_field :last_name, class: 'form-control', placeholder: 'Last name', autofocus: true, autocomplete: "last name" %>
                              </div>
                            </div>
                          </div>


                            <div class="form-group">
                                <label class="form-control-label" for="input-email">Email address</label>
                                <div class="input-group input-group-transparent">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text"><i class="fas fa-user"></i></span>
                                    </div>
                                        <%= f.email_field :email, class: 'form-control', id: 'input-email', placeholder: 'name@example.com', autofocus: true, autocomplete: "email" %>
                                </div>
                            </div>
                            <div class="form-group mb-4">
                                <label class="form-control-label" for="input-password">Password</label>
                                <% if @minimum_password_length %>
                                 <em>(<%= @minimum_password_length %> characters minimum)</em>
                                <% end %><br />
                                <div class="input-group input-group-transparent">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text"><i class="fas fa-key"></i></span>
                                    </div>
                                    <%= f.password_field :password, class:"form-control", id:"input-password", placeholder:"********", autocomplete: "new-password" %>
                                    <div class="input-group-append">
                                        <span class="input-group-text" onclick="showPassword('input-password')">
                                            <i class="fas fa-eye"></i>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="form-control-label" for="input-password-confirm">Confirm password</label>
                                <div class="input-group input-group-transparent">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text"><i class="fas fa-key"></i></span>
                                    </div>
                                        <%= f.password_field :password_confirmation,class:"form-control", id:"input-password-confirm", placeholder:"********", autocomplete: "new-password" %>
                                        <div class="input-group-append">
                                            <span class="input-group-text" onclick="showPassword('input-password-confirm')">
                                                <i class="fas fa-eye"></i>
                                            </span>
                                        </div>
                                </div>
                            </div>
                            <div class="my-4">
                                <div class="custom-checkbox">
                                  <%= f.label :terms_of_service, :class => 'checkbox' do %>
                                    <%= f.check_box :terms_of_service %>
                                    <span style="margin-left:6px">I agree to the <a href="#">Terms and Conditions</a> and <a href="#">Privacy Policy</a>.</span>
                                  <% end %>
                                </div>
                            </div>
                            <div class="row align-items-center">
                                <div class="col-sm-7">
                                       <%= f.submit "Create account", class:"btn btn-primary mb-3 mb-sm-0" %>
                                </div>
                                <div class="col-sm-5 text-sm-right">
                                    <span class="small d-sm-block d-md-inline">Already registered?</span>
                                    <%= link_to "Sign in",'/fr/users/sign_in', style:"small font-weight-bold"%>

                                    <%= link_to("Logout", destroy_user_session_path, :method => :delete)%>

                                    <%= link_to "Sign in with Facebook", '/users/auth/facebook?locale=fr', style:"small font-weight-bold"%>
                                </div>
                                <div class="col-sm-5 text-sm-right">
                                <%= link_to "Sign up debug", '/fr/users/sign_up', style:"small font-weight-bold"%>
                                </div>
                            </div>
                        <% end %>
                    </div>
                </div>
            </div>
